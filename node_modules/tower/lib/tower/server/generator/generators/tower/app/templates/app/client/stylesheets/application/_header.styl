#header
  @include clearfix
  clear: both
  position: relative
  margin-top: 80px
  .frame
    border-left: none
    border-right: none
  #subtitle
    font-size: 36px

#navigation
  position: relative
  .forkme
    position: absolute
  ul
    @include clearfix
    position: relative
    text-align: right
    margin: 0px
    padding: 0px
    float: right
    li
      float: left
      display: block
      margin: 8px 0px 6px 20px
      list-style-type: none